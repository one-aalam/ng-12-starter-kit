{
	"name": "ng-starter-kit",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --proxy-config proxy.conf.js",
		"build": "ng build --configuration production",
		"watch": "ng build --watch --configuration development",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"cy:run": "concurrently \"npm run start\" \"cypress run\"",
		"cy:open": "concurrently \"npm run start\" \"cypress open\"",
		"cy:ci": "concurrently \"npm run start\" \"cypress run TransferProtocol='http' --browser chrome --headless\"",
		"lint": "ng lint && htmlhint \"src\" --config .htmlhintrc",
		"prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
		"prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
		"postinstall": "npm run prettier -s",
		"generate": "ng generate"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^12.0.0",
		"@angular/common": "^12.0.0",
		"@angular/compiler": "^12.0.0",
		"@angular/core": "^12.0.0",
		"@angular/forms": "^12.0.0",
		"@angular/platform-browser": "^12.0.0",
		"@angular/platform-browser-dynamic": "^12.0.0",
		"@angular/router": "^12.0.0",
		"@iconify/icons-ion": "^1.1.2",
		"@nguniversal/express-engine": "^12.0.0",
		"@ngx-translate/core": "^13.0.0",
		"@ngx-translate/http-loader": "^6.0.0",
		"@visurel/iconify-angular": "^11.0.0",
		"concurrently": "^6.1.0",
		"rxjs": "~6.6.0",
		"tslib": "^2.1.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^11.1.1",
		"@angular-devkit/build-angular": "^12.0.0",
		"@angular-eslint/builder": "^12.0.0",
		"@angular-eslint/eslint-plugin": "^12.0.0",
		"@angular-eslint/eslint-plugin-template": "^12.0.0",
		"@angular-eslint/template-parser": "^12.0.0",
		"@angular/cli": "^12.0.0",
		"@angular/compiler-cli": "^12.0.0",
		"@babel/core": "^7.13.13",
		"@babel/preset-env": "^7.13.12",
		"@types/jasmine": "~3.6.0",
		"@types/jest": "^26.0.23",
		"@types/node": "^15.3.0",
		"@typescript-eslint/eslint-plugin": "^4.22.1",
		"@typescript-eslint/parser": "^4.22.1",
		"babel-loader": "^8.2.2",
		"browserlist": "^1.0.1",
		"cypress": "^7.3.0",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"htmlhint": "^0.14.2",
		"https-proxy-agent": "^5.0.0",
		"husky": "^6.0.0",
		"jest": "^26.6.3",
		"jest-preset-angular": "^8.4.0",
		"mini-css-extract-plugin": "^1.5.0",
		"postcss": "^8.2.15",
		"prettier": "^2.3.0",
		"pretty-quick": "^3.1.0",
		"tailwindcss": "^2.1.2",
		"ts-node": "~9.1.1",
		"typescript": "~4.2.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
